{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":disableRateLimiting"
  ],
  "repositories": [
    "TimQaware/test_dependency_Update"
  ],
  "timezone": "Europe/Berlin",
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true,
      "automergeComment": "Auto-Merge By ttttt Renovate",
      "automergeType": "pr",
      "automergeStrategy": "merge-commit"
    }
  ],
  "labels": ["dependencies"]
}
