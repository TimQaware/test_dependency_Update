{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":disableRateLimiting"
  ]
  "timezone": "Europe/Berlin",
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true,
      "automergeComment": "Auto-Merge By Renovate",
      "automergeType": "pr",
      "automergeStrategy": "merge-commit"
    }
  ],
  "Label": ["dependencies"],
}
